<p>Rejestracja użytkownika</p>
<% form_for @user do |f| %>
  <%= f.error_messages %>
  <p>
    Login:<br />
    <%= f.text_field :username %>
  </p>
  <p>
    e-mail:<br />
    <%= f.text_field :email %>
  </p>
  <p>
    Hasło:<br />
    <%= f.password_field :password %>
  </p>
  <p>
    Potwierdzenie hasła:
    <%= f.password_field :password_confirmation %>
  </p>
  <p><%= f.submit 'Zarejestruj' %></p>
<% end %>
