<br />


<% if !@notes.nil? && !@wydarzenia.nil? %>
  <% @wydarzenia.each do |kol| %>

    <% Event.wydarzenia(kol).each do |wydarzenie| %>
    Nazwa lokalizacji: <%= Place.find(kol).title%>

<div id="tresc">
  <div id="<%= Category.find(wydarzenie.category_id).title.downcase %>">
  <li><%# Place.find(wydarzenie.place_id).title %> godz. <%= wydarzenie.start_time.strftime("%H:%M") %></li>
    <p>
      <a onClick="MM_changeProp('nota<%= wydarzenie.id %>','','display','inherit','DIV')"><%= wydarzenie.title %></a>, 
      <% if wydarzenie.price == 'free' || wydarzenie.price == 'f' || wydarzenie.price == ''%>
      <img src="/images/pk/ico_wstep_wolny_short.png" />
        <% else %>
              bilety: <%= wydarzenie.price %> z≈Ç
      <% end %>  
  </p>
    <div id="nota<%= wydarzenie.id %>", class="nota">
      <div id="minus">
        <a onclick="MM_changeProp('nota<%= wydarzenie.id %>','','display','none','DIV')" >
        <img src="/images/pk/arrow_top.png" alt="[-]" />
      </a>
    </div>
    <%= wydarzenie.body %>
    <div id="nota_more">
  <%= link_to "<img src=/images/pk/arrow_more.png>", :controller => pol2ascii(Category.find(wydarzenie.category_id).title.downcase), :action => "show", :id => wydarzenie %>

    </div>
  </div>
 </div>
 </div>
<% end %>
<% end %>
<% end %>















