<h1>New soon</h1>

<% form_for :soon, @soon, :url => {:action => "create"} do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :magazine_id %><br />
    <%= f.text_field :magazine_id, :value => params[:magazine_id] %>
  </p>
  <p>
    <%= f.label :position %><br />
    <%= f.text_field :position %>
  </p>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.submit t('nav.Create') %>
  </p>
<% end %>

<%= link_to 'Back', admin_magazine_soons_path %>